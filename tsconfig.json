{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "strictPropertyInitialization": false,             /* Check for class properties that are declared but not set in the constructor. */

  },

  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
